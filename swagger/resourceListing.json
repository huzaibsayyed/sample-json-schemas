{
    "id": "https://raw.githubusercontent.com/huzaibsayyed/sample-json-schemas/refs/heads/master/swagger/resourceListing.json#",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [ "swaggerVersion", "apis" ],
    "properties": {
        "swaggerVersion": { "enum": [ "1.2" ] },
        "apis": {
            "type": "array",
            "items": { "$ref": "https://raw.githubusercontent.com/huzaibsayyed/sample-json-schemas/refs/heads/master/swagger/resourceObject.json#" }
        },
        "apiVersion": { "type": "string" },
        "info": { "$ref": "https://raw.githubusercontent.com/huzaibsayyed/sample-json-schemas/refs/heads/master/swagger/infoObject.json#" },
        "authorizations": { "$ref": "https://raw.githubusercontent.com/huzaibsayyed/sample-json-schemas/refs/heads/master/swagger/authorizationObject.json#" }
    }
}
